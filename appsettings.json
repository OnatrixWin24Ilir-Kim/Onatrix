{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportOnStartup": true,
      "ExportOnSave": true,
      "ExportAtStartup": false
    },
    "Sets": {
      "Default": {
        "HandlerSets": [
          {
            "Name": "Content",
            "Enabled": true,
            "HandlerDefaults": {
              "Enabled": true
            }
          }
        ]
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "SanitizeTinyMce": true,
        "Id": "c90db309-7691-4ac5-a8c0-9c22e4fe8e63"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=localhost\\SQLEXPRESS;Database=OnatrixDB;Integrated Security=true;TrustServerCertificate=true;"
  }
}