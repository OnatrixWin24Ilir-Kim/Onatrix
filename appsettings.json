{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "SanitizeTinyMce": true,
        "Id": "c90db309-7691-4ac5-a8c0-9c22e4fe8e63"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      }
    },
    "uSync": {
      "Settings": {
        "ImportAtStartup": "All",
        "ExportAtStartup": "None",
        "ExportOnSave": "All",
        "RebuildCacheOnCompletion": true,
        "FailOnMissingParent": false,
        "ImportAfterInstall": true
      },
      "Sets": {
        "Default": {
          "HandlerSets": [
            {
              "Name": "Content",
              "Enabled": true,
              "HandlerDefaults": {
                "Enabled": true
              }
            }
          ]
        }
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=/Users/kimhammerstad/RiderProjects/Onatrix/umbraco/Data/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}
